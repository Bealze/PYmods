<RadialMenu.xml>
<MapMenuKey>KEY_LALT</MapMenuKey>
<MenuReloadHotkey>KEY_LCONTROL+KEY_F11</MenuReloadHotkey>
<HotkeyCommandDelay>250</HotkeyCommandDelay>
<CHAT_DELAY>1200</CHAT_DELAY>

<!-- Настройка меню для танков и пт-->

<TankMenu>
    <CommonCommands>
	        <Command>
				<Title>Вернитесь</Title>
				<Icon>Backtobase</Icon>
				<Text>Вернитесь на базу!!!</Text>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Нужен свет</Title>
				<Icon>Yes</Icon>
				<Text>Подсветите квадрат %(viewPos)s</Text>
				<Ping>%(viewPos)s</Ping>
				<ChatMode>Team</ChatMode>
			</Command>			
			<Command>
				<Title>Перезаряжаюсь!</Title>
				<Icon>Reload</Icon>
				<Text>Перезаряжаюсь, ещё %(reload)s сек.</Text>
				<ChatMode>Team</ChatMode>
				<Hotkey>KEY_C</Hotkey>				
			</Command>
			<Command>
				<Title>Арта помоги</Title>
				<Icon>Helpme</Icon>
				<Text>Арта!!! Помоги мне!!!</Text>
				<Ping>%(ownPos)s</Ping>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Свечу</Title>
				<Icon>Followme</Icon>
				<Text>Свечу!!! Поддержите!!!</Text>
				<Ping>%(viewPos)s</Ping>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Контроль</Title>
				<Icon>No</Icon>
				<Text>Контролируйте направление</Text>
				<Ping>%(viewPos)s</Ping>
				<ChatMode>Team</ChatMode>
            </Command>
    </CommonCommands>
    <TeamCommands>
            <Command>
				<Title>Вперёд</Title>
				<Icon>Followme</Icon>
				<Text>%(name)s, вперёд!!!</Text>
				<ChatMode>Team</ChatMode>	
			</Command>
			
			<Command>
				<Title>Стой</Title>
				<Icon>Stop</Icon>
				<Text>%(name)s, стой!!!</Text>
				<ChatMode>Team</ChatMode>				
			</Command>

			<Command>
				<Title>Из прицела</Title>
				<Icon>Turnback</Icon>
				<Text>%(name)s, мешаешь!!! Уйди из прицела!!!</Text>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Спрячься</Title>
				<Icon>No</Icon>
				<Text>%(name)s, прижмись к укрытию!!!</Text>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Олень</Title>
				<Icon>Attack</Icon>
				<Text>%(name)s, тупой олень!!!</Text>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Назад</Title>
				<Icon>Backtobase</Icon>
				<Text>%(name)s, назад!!!</Text>
				<ChatMode>Team</ChatMode>
			</Command>
    </TeamCommands>
    <EnemyCommands>
			<Command>
				<Title>Под огнём</Title>
				<Icon>Turnback</Icon>
				<Text>%(vehicle)s сводится на меня</Text>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Загуслил</Title>
				<Icon>Attack</Icon>
				<Text>%(vehicle)s стоит на гусле</Text>
				<Ping>%(viewPos)s</Ping>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Перезаряжаюсь</Title>
				<Icon>Reload</Icon>
				<Text>Перезаряжаюсь ещё %(reload)s сек. Светите %(vehicle)s</Text>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>HELP!</Title>
				<Icon>Helpme</Icon>
				<Text>HELP!!! Уничтожьте %(vehicle)s!!!</Text>
				<Ping>%(viewPos)s</Ping>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Выманить!</Title>
				<Icon>Yes</Icon>
				<Text>Выманивайте %(vehicle)s под АРТУ</Text>
				<ChatMode>Team</ChatMode>
			</Command>
			<Command>
				<Title>Добью</Title>
				<Icon>Support</Icon>
				<Text>Свёлся, доберу %(vehicle)s</Text>
				<ChatMode>Team</ChatMode>
			</Command>
	</EnemyCommands>

</TankMenu>

<!-- Настройка меню для АРТ-САУ -->
	
